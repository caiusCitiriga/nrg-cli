{
    "type": "object",
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "srcFolder": {
            "$id": "/properties/srcFolder",
            "type": "string",
            "title": "The Srcfolder Schema ",
            "default": "",
            "examples": [
                "test_src"
            ]
        },
        "defaultExt": {
            "$id": "/properties/defaultExt",
            "type": "string",
            "title": "The Defaultext Schema ",
            "default": "",
            "examples": [
                "ts"
            ]
        },
        "additionalTypes": {
            "$id": "/properties/additionalTypes",
            "type": "array",
            "items": {
                "$id": "/properties/additionalTypes/items",
                "type": "object",
                "properties": {
                    "name": {
                        "$id": "/properties/additionalTypes/items/properties/name",
                        "type": "string",
                        "title": "The Name Schema ",
                        "default": "",
                        "examples": [
                            "rumble"
                        ]
                    },
                    "plural": {
                        "$id": "/properties/additionalTypes/items/properties/plural",
                        "type": "string",
                        "title": "The Plural Schema ",
                        "default": "",
                        "examples": [
                            "rumbles"
                        ]
                    }
                }
            }
        },
        "dotnetInterfaceStyle": {
            "$id": "/properties/dotnetInterfaceStyle",
            "type": "boolean",
            "title": "The Dotnetinterfacestyle Schema ",
            "default": false,
            "examples": [
                true
            ]
        }
    }
}