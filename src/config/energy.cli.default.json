{
    "$schema": "https://raw.githubusercontent.com/caiusCitiriga/nrg-cli/38bd13fb9fd8054793460851cf4b8c75635c3779/src/config/cli-conf.schema.json",
    "srcFolder": "src",
    "defaultExt": "ts",
    "additionalTypes": [
        {
            "name": "test",
            "plural": "tests"
        }
    ],
    "customFileTemplates": [
        {
            "itemName": "dto",
            "templateUrl": "./templates/dto.template.ts",
            "template": "export class #classname# {\n\n}\n"
        },
        {
            "itemName": "test",
            "templateUrl": "./templates/test.template.ts",
            "template": "export class #classname# {\n\n}\n"
        }
    ],
    "dotnetInterfaceStyle": false
}